<%= link_to "Add Friend", new_friendship_path(user_id: current_user, friend_id: params[:id]), class: "nav-link active bg-primary bg-gradient text-white", data_toggle: "tab", role: "tab", aria_controls: "tasks", aria_selected: "true"%>

<%= simple_form_for :gruppetto, url: gruppettos_path, method: :get, html: { class: 'form-inline' } do |f| %>
  <%= f.input :type, "test"%>
